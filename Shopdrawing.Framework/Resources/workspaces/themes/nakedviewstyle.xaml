<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:vs="clr-namespace:Microsoft.VisualStudio.PlatformUI.Shell.Controls"
                    x:Uid="ResourceDictionary_1">
  <Style x:Uid="Style_1" TargetType="{x:Type vs:NakedViewControl}">
    <Setter x:Uid="Setter_1" Property="Control.Template">
      <Setter.Value>
        <ControlTemplate x:Uid="ControlTemplate_1" TargetType="{x:Type vs:NakedViewControl}">
          <vs:DockTarget x:Uid="vs:DockTarget_1" DockTargetType="SidesOnly" Border.Background="Transparent">
            <ContentPresenter x:Uid="ContentPresenter_1" Content="{TemplateBinding ContentControl.Content}"/>
          </vs:DockTarget>
        </ControlTemplate>
      </Setter.Value>
    </Setter>
  </Style>
</ResourceDictionary>