<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:vs="clr-namespace:Microsoft.VisualStudio.PlatformUI.Shell.Controls"
                    x:Uid="ResourceDictionary_1">
  <Style x:Uid="Style_1" TargetType="{x:Type vs:ExpressionDockPreviewWindow}">
    <Setter x:Uid="Setter_4" Property="Control.Template">
      <Setter.Value>
        <ControlTemplate x:Uid="ControlTemplate_1" TargetType="{x:Type vs:ExpressionDockPreviewWindow}">
          <Border x:Name="InsertionBorder" x:Uid="InsertionBorder"
                  Background="{DynamicResource DockAdornerPreviewWindowFillBrush}"
                  BorderBrush="{DynamicResource DockAdornerInsertionBrush}"/>
          <ControlTemplate.Triggers>
            <Trigger x:Uid="Trigger_1" Property="vs:ExpressionDockPreviewWindow.IsFillPreview" Value="true">
              <Setter x:Uid="Setter_6" TargetName="InsertionBorder" Property="Border.Background">
                <Setter.Value>
                  <x:NullExtension/>
                </Setter.Value>
              </Setter>
              <Setter x:Uid="Setter_7" TargetName="InsertionBorder" Property="Border.BorderThickness" Value="2"/>
            </Trigger>
          </ControlTemplate.Triggers>
        </ControlTemplate>
      </Setter.Value>
    </Setter>
  </Style>
</ResourceDictionary>