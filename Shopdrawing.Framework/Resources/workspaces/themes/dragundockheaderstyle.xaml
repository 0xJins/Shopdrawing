<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:vs="clr-namespace:Microsoft.VisualStudio.PlatformUI.Shell.Controls"
                    x:Uid="ResourceDictionary_1">
  <Style x:Uid="Style_1" TargetType="{x:Type vs:DragUndockHeader}">
    <Setter x:Uid="Setter_2" Property="Control.Background" Value="Transparent"/>
    <Setter x:Uid="Setter_3" Property="UIElement.Focusable" Value="false"/>
    <Setter x:Uid="Setter_1" Property="Control.Template">
      <Setter.Value>
        <ControlTemplate x:Uid="ControlTemplate_1" TargetType="{x:Type vs:DragUndockHeader}">
          <Border x:Uid="Border_1" Background="{TemplateBinding Control.Background}">
            <ContentPresenter x:Uid="ContentPresenter_1" ContentSource="Content"/>
          </Border>
        </ControlTemplate>
      </Setter.Value>
    </Setter>
  </Style>
</ResourceDictionary>