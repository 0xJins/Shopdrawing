            </Setter.Value>
        </Setter>
    </Style>
    <Style x:Key="GalleryItemStyle" x:Uid="Style_7" TargetType="{x:Type Controls:SelectingListBoxItem}"
         BasedOn="{StaticResource ExpressionListBoxItem}">
