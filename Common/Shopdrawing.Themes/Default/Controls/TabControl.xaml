                <Setter x:Uid="Setter_1045" Property="Control.Background" Value="{DynamicResource PaletteBrush}"/>
                <Setter x:Uid="Setter_1046" Property="Control.Foreground" Value="{DynamicResource DisabledTextBrush}"/>
            </Trigger>
